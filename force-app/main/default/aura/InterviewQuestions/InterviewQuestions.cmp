<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes" controller="InterviewQuestions">
    <aura:attribute name="questionMap" type="Map"/>
    <aura:attribute name="meetings" type="Meeting__c[]"/>
    <aura:handler name="init" action="{!c.doInit}" value="{!this}"/>
    
    <lightning:select label="Meetings">
        <aura:iteration items="{!v.meetings}" var="meet">
        	<option>{!meet.Name}</option>
        </aura:iteration>   
    </lightning:select>
</aura:component>